	    <h1> User Page </h1>
	<!--renders a page that displays all your users-->
	    <p><strong> A list of all users </strong></p>
	        
	        <% parsedData.forEach (function (usersname){ %>
	     	  
	            <p>First Name: <%= usersname.firstname %></p>
	            <p>Last Name: <%= usersname.lastname %></p>
	            <p>Email: <%= usersname.email %></p> <br>
	          
	    <% }); %>

	</body>
